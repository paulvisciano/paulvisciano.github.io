# Environment Configuration Template
# Copy this to .env and fill in your actual values
# .env is git-ignored and never committed

# GitHub
GITHUB_TOKEN=

# ChatGPT/OpenAI (if using API)
OPENAI_API_KEY=

# Cloudflare R2 (if using image hosting)
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=

# Google Maps Timeline (local export path)
GOOGLE_TIMELINE_PATH=

# Local development
NODE_ENV=development
